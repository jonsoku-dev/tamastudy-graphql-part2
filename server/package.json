{
  "name": "typescript-graphql-part2",
  "version": "1.0.0",
  "description": "tamastudy grapqhl part2",
  "main": "app.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/app.js",
    "watch-node": "nodemon dist/app.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "dev": "cd src && nodemon --exec ts-node app.ts -e ts",
    "precodegen": "gql-merge --out-file ./src/schema.graphql ./src/api/**/*.graphql",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "author": "JONGSEOK LEE",
  "license": "MIT",
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.3",
    "@graphql-codegen/introspection": "1.17.3",
    "@graphql-codegen/typescript": "^1.17.3",
    "@graphql-codegen/typescript-document-nodes": "1.17.3",
    "@graphql-codegen/typescript-mongodb": "^1.17.3",
    "@graphql-codegen/typescript-resolvers": "1.17.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.27",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "4.0.0",
    "gql-merge": "^0.0.6",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "apollo-server-express": "^2.15.0",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.1.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.8",
    "mongoose": "^5.9.19",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6"
  }
}
